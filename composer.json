{
    "name": "elinkcms/elink-laravel-file-manager",
    "description": "File manager for Laravel",
    "keywords": [
        "laravel",
        "file",
        "manager",
        "elink",
        "cms",
        "ecommerce"
    ],
    "authors": [
        {
            "name": "Dmitry Falkovich",
            "email": "elink.cms.laravel@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/elinkcms/elink-laravel-file-manager",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "ext-zip": "*",
        "ext-json": "*",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0",
        "league/flysystem": "^1.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Elinkcms\\ElinkLaravelFileManager\\": "src"
        }
